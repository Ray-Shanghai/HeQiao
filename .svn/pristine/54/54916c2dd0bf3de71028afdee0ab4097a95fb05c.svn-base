/**
 * 公共样式
 */

 /**头部 header**/
 .content{
 	margin: 0 auto;
 }
 .public-header-warrp{
    width: 100%;
    top:0;
    left:0;
    z-index: 10000;
 }
 .public-header{
 	height: 70px;
 	background: #C9EAF7;
    width: 100%;
   }
 .public-header .public-header-logo img{
    height: 44px;
 	margin-top:13px;
	margin-left:40px;
 	float: left;
	display:inline-block;
 }
 .public-header .public-header-logo a{
 	color:#000;
 }
 .public-header .public-header-logo h3{
 	line-height: 43px;
    padding: 0px;
    margin: 0px;
    font-weight: normal;
    padding-left: 15px;
    font-size: 20px;
    display: inline;
 }
 .public-header .public-header-logo i{
 	display: inline-block;
 	width:50px;
 	height: 50px;
 	text-align: center;
 	color:#FFF;
 	line-height: 50px;
 	background: #6bc095;
 }
 .public-header .public-header-admin{
 	min-width: 300px;
 	line-height: 70px;
	margin-right:48px;
	display:inline-block;
    font-family: 幼圆;
    font-weight: bold;
    font-size: 20px;
    color: #0098c5;
    padding-top: 10px;
 }
 .public-header .public-header-admin .admin-name{
 	display: inline-block;
    width: 176px;
 }
 .public-header .public-header-admin a{
 	display: inline;
 	color:#fefefe;
 	padding: 8px;
 	border-radius: 8px;
 }
 .public-header .public-header-admin .public-header-man{
 	background:#a5cf4c;
 }
 .public-header .public-header-admin .public-header-loginout{
 	background:#52afb7;
 }
 /*公共头部end*/

 /********************************页面内容展示*****************************************/
.public-ifame .content{
	background: #fefefe;
    position:  absolute;
    left:0px;
 	width:100%;
}
 /*头部*/
 .public-ifame .public-ifame-header{
	height:70px;
	background: #f4f5f9;
    border-bottom: 1px solid #ddd;
    margin-bottom: 15px;
 }
 .public-ifame .public-ifame-header .ifame-item{
 	height:64px;
 	width:30%;
 	float:left;
 	overflow: hidden;
 }
 .public-ifame .public-ifame-header .ifame-item .item-warrp{
 	height: 60px;
 	border-right:1px solid #DDD;
 	padding: 10px;
 }
 .public-ifame .public-ifame-header .logo i{
 	display: inline-block;
    width: 50px;
    float: left;
    height: 50px;
    text-align: center;
    color: #FFF;
    line-height: 50px;
    background: #f4e039;

 }
 .public-ifame .public-ifame-header .ifame-item .logo-title{
 	font-size: 18px;
    display: inline-block;
    margin-left: 16px;
    height: 30px;
    line-height: 30px;
 }
 .public-ifame .public-ifame-header .ifame-item .logo-des{
 	font-size: 12px;
 	color:#999;
 	margin-top: 6px;
 	text-align: right
 }
.public-ifame .public-ifame-header .ifame-item span {
    font-size: 12px;
    line-height: 20px;
}
.public-ifame .public-ifame-leftnav{
	width: 150px;
    float: left;
    background: #f5f5f5;
    position:relative;
 }
.public-ifame .public-ifame-leftnav .public-title-warrp{
    border-bottom:1px solid #ddd;
}
.public-ifame .public-ifame-leftnav .public-ifame-title{
    height: 50px;
    line-height: 50px;
    background: url("../images/home.png") center left no-repeat;
    padding-left: 24px;
    font-size: 16px;
    font-weight: 600;
    margin-left: 15px;
}
.public-ifame .public-ifame-leftnav .left-nav-list{
    margin-top: 10px;
}
.public-ifame .public-ifame-leftnav .public-ifame-item{
	/*height:36px;*/
	line-height: 30px;
    padding-left:20px;
    position: relative;
    margin-bottom:20px;
}
.public-ifame .public-ifame-leftnav .public-ifame-item a{
	color:#0098c5;
    font-size: 16px;
    font-weight: bold;
}
.ifame-item-sub a{
    color: #716666!important;
    font-size: 13px!important;
}
.ifame-item-sub ul li a{font-weight:normal !important;}
.public-ifame .public-ifame-leftnav .ifame-item-sub li{
    height:30px;
    line-height: 30px;
    color:#888;
}
 
 
/*内容iframe*/
.public-ifame-content{
    width:85%;
    margin-left: 190px; 
    background: #fff;
}
.loginInfo{
    line-height: 20px;
    font-size: 12px;
    color: #595656;
    position:absolute;
    right:20px;
    bottom:30px;
    text-align:right;
}
.loginInfo a{color:#0098c5; font-weight:bold;}