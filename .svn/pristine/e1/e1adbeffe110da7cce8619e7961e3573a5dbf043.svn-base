@charset "utf-8";
/*百分比栅格布局*/
*{box-sizing: border-box;}
body,html{width: 100%; height: 100%;}
a:hover,a:active{-webkit-tap-highlight-color:transparent;}
.text-center{text-align: center;}
.text-right{text-align: right;}
.clf:after{content:"";display:block;height:0;line-height: 0;clear:both;}
.position-rel{position:relative;}
.row{display: block;height:auto;}
.row [class*=col-]{display: block;float:left;}
.row:after{content:"";display:block;height:0;line-height: 0;clear:both;}
.row .col-1{width:8.333333%;}
.row .col-2{width:16.666667%;}
.row .col-20{width:20%;}
.row .col-3{width:25%;}
.row .col-4{width:33.333333%;}
.row .col-5{width:41.666667%;}
.row .col-6{width:50%;}
.row .col-7{width:58.333333%;}
.row .col-8{width:66.666667%;}
.row .col-9{width:75%;}
.row .col-10{width:83.333333%;}
.row .col-11{width:91.666667%;}
.row .col-12{width:100%;}
.row .col-22{width:22%;}
.row .col-78{width:78%;}
.iconfont{font-size: 0.16rem;}
.block-a{display:block;}
/*按钮*/
.md-blue-btn{width:100%;height:0.7rem;line-height:0.68rem;background:#0098c5;font-size:0.32rem;color:#fff !important;border:1px solid #0098c5;border-radius:0.04rem; display: block; text-align: center; color: #fff;}
.md-blue-btn-radnone{width:100%;height:0.7rem;line-height:0.68rem;background:#0098c5;font-size:0.32rem;color:#fff !important;border:1px solid #0098c5;display: block; text-align: center; color: #fff;}
.md-black-btn-radnone{width:100%;height:0.7rem;line-height:0.68rem;background:#000;font-size:0.32rem;color:#fff !important;border:1px solid #000;display: block; text-align: center; color: #fff;}
.md-blue-border-btn{width:100%;height:0.7rem;line-height:0.68rem;font-size:0.32rem;color:#0098c5 !important;border:1px solid #0098c5;border-radius:0.04rem; display: block; text-align: center; color: #fff;}

span.gt{font-family:"宋体";font-weight: bold;}
.t-border{border-top:1px solid #dddddd;}
.b-border{border-bottom:1px solid #dddddd;}
.l-border{border-left:1px solid #dddddd;}
.r-border{border-right:1px solid #dddddd;}
.t-dashed-border{border-top:1px dashed #dddddd;}
.b-dashed-border{border-bottom:1px dashed #dddddd;}
.tb-dashed-border{border-bottom:1px dashed #dddddd;border-top:1px dashed #dddddd;}
.ellipsis{overflow: hidden;white-space:nowrap;text-overflow:ellipsis;}
.ellipsis-2{overflow:hidden;text-overflow:ellipsis;display:-webkit-box !important;-webkit-line-clamp: 2;-webkit-box-orient: vertical;}



/*登录*/
.logo{text-align: center;}
.logo img{width: 4rem; margin: 0.5rem;}
.login-from{width: 5rem; margin: 3rem auto 0;}
.login-from .input-a input{width: 100%; height: 0.8rem; line-height: 0.8rem; margin-bottom: 0.15rem; padding-left: 0.7rem;border: 1px solid #ddd;}
.login-from .button-login button{width: 100%; height: 0.76rem; border: none; background: #0098c5; font-size: 0.36rem; color: #fff; border-radius:0.08rem; margin-top: 0.3rem;}
.login-from .input-a{position: relative;}
.login-from .input-a .iconfont{position: absolute; top:0.16rem; left:0.2rem; font-size: 0.36rem; color: #ccc;}
/*首页*/
.content{padding: 0 0.5rem;}
.detail-class .title{font-size: 0.36rem; font-weight: bold; color: #0098C5; line-height: 0.7rem;}
.detail-class .notice ul li{width: 100%; height: 0.6rem; line-height: 0.6rem; font-size: 0.24rem; background: #a5dded;}
.detail-class .notice ul li a{padding-left: 0.2rem; width: 3.8rem; display: block;}
.apply .info-all{font-size: 0.24rem; padding-bottom: 0.05rem;}
.person-info{background: #a5dded; padding: 0.2rem 0.15rem; font-size: 0.2rem;}
.footer{position: fixed;bottom: 0; left: 0; width: 100%; height: 0.8rem; background: #000; z-index: 10;}
.footer .row .col-6{height: 0.8rem;text-align: center; line-height: 0.8rem;}
.footer .row .col-6 a{font-size:0.28rem; color: #fff; display: block;}
.footer .row .col-6 .iconfont{font-size: 0.28rem; padding-right: 0.1rem;}
.footer .row .col-4{height: 0.8rem;text-align: center; line-height: 0.8rem;}
.footer .row .col-4 a{font-size:0.28rem; color: #fff; display: block;}
.footer .row .col-4 .iconfont{font-size: 0.28rem; padding-right: 0.1rem;}
.tip-tc{width: 100%; height: 100%; background: #ecfbff; position: fixed; top:0; left: 0; z-index: 999; padding:0 0.5rem;display: none;}
.tip-tc .tip-head{font-size: 0.28rem; margin: 0.3rem 0; color: red;}
.word-ct textarea{width: 100%; height: 6rem; padding: 0.05rem 0.1rem; resize: none;}

/*公告页*/
.detail-content .title{font-size: 0.36rem; color: #0098C5; font-weight: bold; text-align: center;}
.send-date{font-size: 0.20rem; color: #666; margin-top: 0.1rem;}
.all-content{font-size: 0.20rem; color: #333; line-height: 0.4rem; margin-top: 0.2rem;}
/*申请详情页*/
.supply-detail-content .title{width: 100%;background: #a5dded; font-size: 0.32rem; font-weight: bold; color: #0098C5; padding-left: 0.3rem; padding: 0.1rem 0 0.1rem 0.3rem;}
.supply-detail-content .title p{color: #666; font-size: 0.24rem; padding-top: 0.1rem;}
.serial-number{font-size: 0.24rem; color: #0098C5; line-height: 0.5rem;}
.table-one{width: 100%; padding: 0 0.3rem;}
.table-one table{width: 100%; font-size: 0.28rem; border-top: 1px solid #a5dded;border-left: 1px solid #a5dded;}
.table-one table td{border-bottom:1px solid #a5dded; border-right:1px solid #a5dded; padding: 0.2rem 0; padding-left: 0.1rem;}
.table-one .info-title{font-weight: bold; color: #0098C5;}
.table-two{width: 100%;}
.table-two table{width: 100%;font-size: 0.24rem; }
.table-two table thead{background:#a5dded;}
.table-two table thead td{height: 0.46rem; padding: 0 0 0 0.15rem;}
.table-two table tbody td{padding:0.1rem 0 0.1rem 0.15rem;}
.table-three{width: 100%;}
.table-three table{width: 100%; font-size: 0.24rem;}
.table-three table td{padding: 0.2rem 0; padding-left: 0.1rem; text-align: center;}
.table-four{width: 100%; padding: 0.2rem 0.3rem; background: #A5DDED;}
.table-four table{width: 100%; font-size: 0.24rem;}
.table-four table td{padding: 0.2rem 0; padding-left: 0.1rem;}
.table-four table thead td{font-weight: bold; padding: 0 0 0 0.3rem;}
.post-people{font-weight: bold; padding-bottom: 0.1rem;}

































/*设置头像页面*/
.set-photo{height:1.5rem;}
.setting-left{width:2rem;float:left;color:#333;font-size: 0.32rem;}
.setting-right{width:4.7rem;float:right;color:#999;font-size: 0.28rem;text-align:right;}
.setting-right .gt{margin-left:0.26rem;}
.line-h-150{line-height:1.5rem;}
.line-h-100{line-height:1rem;}
.person-photo{display:inline-block;width:1.14rem;height:1.14rem;border-radius:50%;vertical-align:middle;}
.person-photo img{width:1.14rem;height:1.14rem;border-radius:50%;vertical-align:top;}
/*头像设置弹层*/
.modify-info .layui-m-layercont{color:#FD482C;font-size:0.34rem;}
.modify-info .layui-m-layer-footer .layui-m-layerbtn span[no] {color: #007aff;border-top: 1px solid #c2c2c2;border-radius: 0 0 0.05rem 0.05rem;font-size:0.34rem;}
.modify-info .layui-m-layerbtn span[yes] {color: #007aff;}
/*详情页公共部分*/
.sub-top{height:2.1rem;padding:0.3rem 0.4rem;box-shadow: 0rem 0.01rem 0.04rem #ddd;}
.line-title{width:0.8rem;border:1px solid #ff6600;height:1.5rem;padding:0.1rem 0.21rem;font-size:0.36rem;color:#f60;line-height:0.42rem;}
.line-title-green{width:0.8rem;border:1px solid #629b00;height:1.5rem;padding:0.1rem 0.21rem;font-size:0.36rem;color:#629b00;line-height:0.42rem;}
.subTop-title{font-weight: normal;font-size:0.36rem;line-height:0.38rem;}
.subTop-content{font-size: 0.3rem;color:#999;line-height:0.3rem;}
.subContent-first{padding:0.4rem 0.4rem 0.25rem;}
.subContent-info{color:#999;font-size:0.3rem;}
.subContent-sub{padding:0.25rem 0.4rem 0.1rem;}
.subContent-info .col-3{text-align: right;}
.price-row{height:1.2rem;line-height:1.18rem;border-top:1px solid #f90;border-bottom:1px solid #f90;background:#fff5ee;margin-bottom:0.3rem;}
.price-row .col-9{font-size:0.36rem;color:#ff6633;}
.purchase-personinfo{font-size:0.24rem;color:#999;padding:0 0.4rem;margin-bottom:0.7rem;}
.font-image{
	width: 0.32rem;
    vertical-align: sub;
    margin-right: 0.1rem;
}
 .tip-tc1{ width: 100%;
    height: 100%;
    background: #ecfbff;
    position: fixed;
    top: 0;
    left: 0;
    z-index: -1;
    padding: 0 0.5rem;
    opacity:0;
     }
     .signop{
       opacity:1!important;
       z-index:999
     }